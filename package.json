{
  "name": "vue-ele-form-generator",
  "version": "1.3.1",
  "description": "vue-ele-form-generator是专为vue-ele-form开发的可视化表单设计工具, 让表单开发的效率更上一层楼",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "node bin/fgen",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --mode production",
    "release": "standard-version",
    "pubs": "yarn build && yarn release && npm publish --registry http://registry.npmjs.org && git push --follow-tags origin master"
  },
  "bin": {
    "fgen": "./bin/fgen"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dream2023/vue-ele-form-generator"
  },
  "keywords": [
    "vue-ele-form",
    "vue-ele-form-generator",
    "form-schema",
    "vue-form"
  ],
  "homepage": "https://vue-ele-form-generator.netlify.com/",
  "dependencies": {
    "serve-handler": "^6.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "async-validator": "^3.2.3",
    "babel-eslint": "^10.0.1",
    "clipboard-copy": "^3.1.0",
    "commitizen": "^4.0.3",
    "core-js": "2.6.5",
    "cz-conventional-changelog": "^3.0.2",
    "element-ui": "^2.13.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "secure-ls": "^1.2.6",
    "serialize-javascript": "^2.1.2",
    "standard-version": "^7.0.1",
    "vue": "^2.6.10",
    "vue-ele-form": "^0.6.19",
    "vue-ele-form-bmap": "^0.1.0",
    "vue-ele-form-codemirror": "^0.1.0",
    "vue-ele-form-dynamic": "^0.1.1",
    "vue-ele-form-gallery": "^0.1.1",
    "vue-ele-form-image-uploader": "^0.1.4",
    "vue-ele-form-json-editor": "^0.1.2",
    "vue-ele-form-markdown-editor": "^0.1.1",
    "vue-ele-form-quill-editor": "^0.1.0",
    "vue-ele-form-table-editor": "^0.1.2",
    "vue-ele-form-tree-select": "^0.1.1",
    "vue-ele-form-upload-file": "0.0.5",
    "vue-ele-form-video-uploader": "^0.1.3",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  }
}
